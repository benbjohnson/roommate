<%! func (t *RoomTemplate) New(w io.Writer) error %>

<html>
<% t.Head(w) %>

<body>
  <h1>Roommate</h1>

  <h2>New Room</h2>

  <form method="POST" action="/rooms">
    <p>
      <label>Name</label>
      <input type="text" name="name"/>
    </p>
    <p>
      <input type="submit" value="Create Room"/>
    </p>
  </form>
</body>
</html>
